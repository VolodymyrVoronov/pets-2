generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Pet {
  id          Int      @id @default(autoincrement())
  name        String
  age         Int
  breed       String
  photo       String
  diet        String
  diseases    String
  information String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
